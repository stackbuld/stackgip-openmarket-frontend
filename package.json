{
  "name": "stackbuld-ecommerce",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:without-ssr": "ng serve",
    "start": "yarn build && yarn run dev:ssr",
    "build-dev": "ng build",
    "build-prod": "cross-env BUILD_ENV=$BUILD_ENV node select-build-script.js",
    "build": "yarn run build:ssr",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run stackbuld-ecommerce:serve-ssr",
    "serve:ssr": "node dist/stackbuld-ecommerce/server/main.js",
    "prerender": "ng run stackbuld-ecommerce:prerender --no-guess-routes --routes / ",
    "build-dev:ssr": "node set-env.js && ng build --configuration development && ng run stackbuld-ecommerce:server:development",
    "build:ssr": "node set-env.js && ng build --configuration production && ng run stackbuld-ecommerce:server:production",
    "build:ssr-with-prerender": "ng build --configuration production && ng run stackbuld-ecommerce:server:production && yarn run prerender",
    "vercel-build": "yarn run build-prod"
  },
